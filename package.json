{
	"name": "nuxt-admin-template",
	"version": "1.0.0",
	"description": "admin template with nuxtjs",
	"author": "ucaesar &amp; prometheuszyx",
	"private": true,
	"scripts": {
		"dev": "set TS_NODE_COMPILER_OPTIONS={\"module\":\"commonjs\"}&& cross-env NODE_ENV=development node --nolazy -r ts-node/register server/start.ts",
		"dev-mon": "set TS_NODE_COMPILER_OPTIONS={\"module\":\"commonjs\"}&& cross-env NODE_ENV=development nodemon --watch server --exec ts-node server/start.ts",
		"build": "nuxt-ts build",
		"start": "cross-env NODE_ENV=production node server/index.js",
		"generate": "nuxt generate",
		"lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
		"test": "set TS_NODE_COMPILER_OPTIONS={\"module\":\"commonjs\"}&& mocha -r ts-node/register --colors",
		"test-server": "node server/test/start.js",
		"server": "nuxt-ts",
		"product": "nuxt-ts start",
		"mock": "cross-env NODE_ENV=development nodemon server/mock.js --watch server",
		"prettier": "prettier --ignore-path \"node_modules/**\" --write \"./**/*.vue\""
	},
	"config": {
		"nuxt": {
			"host": "0.0.0.0",
			"port": "3000"
		}
	},
	"dependencies": {
		"@nuxt/typescript-runtime": "^0.3.8",
		"@nuxtjs/axios": "^5.3.6",
		"@nuxtjs/pwa": "^3.0.0-beta.19",
		"@paypal/checkout-server-sdk": "^1.0.2",
		"@typescript-eslint/eslint-plugin": "^2.19.2",
		"@typescript-eslint/parser": "^2.19.2",
		"body-parser": "^1.19.0",
		"casbin": "^3.0.8",
		"casbin-sequelize-adapter": "^2.0.0",
		"cross-env": "^7.0.0",
		"crypto": "^1.0.1",
		"delivery-tracker": "^2.4.2",
		"express-session": "^1.17.0",
		"fedex-manager": "^1.0.1",
		"fedex-nodejs": "^1.1.0",
		"koa": "^2.11.0",
		"koa-bodyparser": "^4.2.1",
		"koa-router": "^7.4.0",
		"koa-session": "^5.12.3",
		"koa-static": "^5.0.0",
		"lodash": "^4.17.19",
		"mysql2": "^1.7.0",
		"node-shipping-fedex": "^1.0.0",
		"nuxt": "^2.11.0",
		"nuxt-i18n": "^6.3.0",
		"nuxt-property-decorator": "^2.5.0",
		"paypal-rest-sdk": "^2.0.0-rc.2",
		"sequelize": "^5.19.6",
		"sequelize-typescript": "^1.0.0",
		"sequelizejs-decorators": "^5.0.0",
		"sqlite3": "^4.1.0",
		"ts-babel": "^6.1.7",
		"ts-node": "^8.4.1",
		"validator": "^12.1.0",
		"vee-validate": "^3.2.3",
		"vue-pdf": "^4.1.0",
		"vuex-module-decorators": "^0.10.1",
		"xml2js": "^0.4.23"
	},
	"devDependencies": {
		"@babel/register": "^7.9.0",
		"@nuxt/types": "^0.3.2",
		"@nuxt/typescript-build": "^0.5.6",
		"@nuxtjs/eslint-config": "^2.0.2",
		"@nuxtjs/eslint-module": "^1.1.0",
		"@nuxtjs/style-resources": "^1.0.0",
		"@nuxtjs/vuetify": "^1.11.2",
		"@types/bluebird": "^3.5.28",
		"@types/chai": "^4.2.4",
		"@types/connect": "^3.4.32",
		"@types/koa": "^2.0.51",
		"@types/koa-bodyparser": "^4.3.0",
		"@types/koa-router": "^7.0.42",
		"@types/koa-session": "^5.10.1",
		"@types/koa-static": "^4.0.1",
		"@types/lodash": "^4.14.144",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.5",
		"@types/validator": "^10.11.3",
		"babel-eslint": "^10.0.1",
		"chai": "^4.2.0",
		"chai-http": "^4.3.0",
		"connect": "^3.7.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.10.0",
		"eslint-config-standard": "^14.1.0",
		"eslint-plugin-html": "^6.0.0",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-jest": "^22.20.0",
		"eslint-plugin-node": "^10.0.0",
		"eslint-plugin-nuxt": ">=0.5.1",
		"eslint-plugin-prettier": "^3.1.2",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-standard": "^4.0.1",
		"eslint-plugin-vue": "^5.2.3",
		"mocha": "^6.2.1",
		"nodemon": "^1.18.9",
		"prettier": "^1.19.1",
		"prettier-eslint": "^9.0.0",
		"vue-class-component": "^7.1.0",
		"vue-property-decorator": "^8.3.0"
	}
}
