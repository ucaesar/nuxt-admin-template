{
	"name": "nuxt-admin-template",
	"version": "1.0.0",
	"description": "admin template with nuxtjs",
	"author": "ucaesar &amp; prometheuszyx",
	"private": true,
	"scripts": {
		"dev": "cross-env NODE_ENV=development nodemon --watch server --exec ts-node server/start.ts",
		"build": "nuxt build",
		"start": "cross-env NODE_ENV=production node server/index.js",
		"generate": "nuxt generate",
		"lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
		"test": "mocha -r ts-node/register --colors",
		"test-server": "node server/test/start.js",
		"server": "nuxt-ts",
		"mock": "cross-env NODE_ENV=development nodemon server/mock.js --watch server"
	},
	"dependencies": {
		"@nuxt/types": "^0.3.2",
		"@nuxt/typescript-runtime": "^0.2.2",
		"@nuxtjs/axios": "^5.3.6",
		"@types/lodash": "^4.14.144",
		"casbin": "^3.0.4",
		"casbin-sequelize-adapter": "^2.0.0",
		"cross-env": "^5.2.0",
		"crypto": "^1.0.1",
		"koa": "^2.8.1",
		"koa-bodyparser": "^4.2.1",
		"koa-router": "^7.4.0",
		"koa-session": "^5.12.3",
		"koa-static": "^5.0.0",
		"lodash": "^4.17.15",
		"mysql2": "^1.7.0",
		"nuxt": "^2.0.0",
		"nuxt-i18n": "^6.3.0",
		"sequelize": "^5.18.4",
		"sqlite3": "^4.1.0",
		"ts-node": "^8.4.1",
		"vue-i18n": "^8.14.1"
	},
	"devDependencies": {
		"@nuxt/typescript-build": "^0.3.2",
		"@nuxtjs/eslint-config": "^1.0.1",
		"@nuxtjs/eslint-module": "^1.0.0",
		"@nuxtjs/vuetify": "^1.0.0",
		"@types/mocha": "^5.2.7",
		"@typescript-eslint/eslint-plugin": "^2.4.0",
		"babel-eslint": "^10.0.1",
		"chai": "^4.2.0",
		"chai-http": "^4.3.0",
		"eslint": "^6.5.1",
		"eslint-config-prettier": "^6.4.0",
		"eslint-plugin-html": "^6.0.0",
		"eslint-plugin-nuxt": ">=0.4.2",
		"eslint-plugin-prettier": "^3.1.1",
		"mocha": "^6.2.1",
		"nodemon": "^1.18.9",
		"prettier": "^1.18.2",
		"prettier-eslint": "^9.0.0"
	}
}
